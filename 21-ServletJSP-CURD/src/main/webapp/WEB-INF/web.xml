<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>StudentEditShowServlet</servlet-name>
		<display-name>StudentEditShowServlet</display-name>
		<description></description>
		<servlet-class>com.vj.servlets.StudentEditShowServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>StudentUpdateServlet</servlet-name>
		<display-name>StudentUpdateServlet</display-name>
		<description></description>
		<servlet-class>com.vj.servlets.StudentUpdateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StudentEditShowServlet</servlet-name>
		<url-pattern>/edit</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>StudentUpdateServlet</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>

</web-app>
